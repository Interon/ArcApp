<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="110" id="co.za.interon.arcapp" ios-CFBundleVersion="1100" version="1.0.110" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
