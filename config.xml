  <name>ArcApp</name>
  <description>
       Arcfyre Application
    </description>
  <author email="anton@interon.co.za" href="www.interon.co.za">
      Interon
    </author>
  <gap:config-file parent="/manifest" platform="android">
    <!--<application android:debuggable="true"/>-->
